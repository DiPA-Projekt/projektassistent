{
  "name": "@dipa-projekt/projektassistent",
  "version": "1.0.0-beta.1",
  "scripts": {
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "e2e": "nightwatch",
    "format": "prettier --check \"{src,tests}/**\" --ignore-unknown",
    "lint:eslint": "eslint {src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}",
    "lint:eslint:ci": "npm run lint:eslint -- --quiet=true -f json -o eslint-report.json || exit 0",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
    "reinstall": "npm run remove && npm install",
    "remove": "rm -rf node_modules package-lock.json",
    "test": "cross-env NODE_ENV=test mocha",
    "update": "ncu -t minor -u && npm install && ncu",
    "vite:build": "vite build",
    "vite:serve": "vite",
    "vite:start": "npm run vite:serve -- --open",
    "webpack:build": "cross-env NODE_ENV=production webpack",
    "webpack:serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
    "webpack:start": "npm run webpack:serve -- --open=chrome",
    "build": "cross-env NODE_ENV=production webpack",
    "build:ci": "npm run format && npm run lint:eslint:ci && npm run coverage && npm run build",
    "serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map --no-hot",
    "start": "npm run serve -- --open",
    "preview": "vite preview",
    "cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require cucumber.conf.js --require tests/cucumber/step-definitions --format @cucumber/pretty-formatter --format json:.reports/cucumber/report.json",
    "prepare:husky": "husky install",
    "commit-msg": "commitlint",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "5.0.1",
    "@dipa-projekt/projektassistent-openapi": "^0.2.4",
    "@leanup/form": "1.3.43",
    "@leanup/lib": "1.3.43",
    "@leanup/openapi-demo": "1.3.41",
    "@public-oss/kolibri-themes": "0.0.2",
    "@public-ui/components": "1.1.16",
    "@public-ui/core": "1.1.16",
    "@public-ui/themes": "1.1.16",
    "antd": "^5.3.0",
    "axios": "^0.25.0",
    "caniuse-lite": "^1.0.30001452",
    "dayjs": "1.10.4",
    "fast-xml-parser": "^4.0.3",
    "he": "^1.2.0",
    "html-react-parser": "1.2.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "navigo": "8.11.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^12.2.0",
    "react-router": "6.9.0",
    "react-router-dom": "6.9.0",
    "react-xml-parser": "github:Bock4Soft/xml-parser",
    "rxjs": "7.8.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@leanup/cli": "1.3.43",
    "@leanup/cli-react": "1.3.43",
    "@leanup/git-hooks": "1.3.43",
    "@leanup/stack": "1.3.43",
    "@leanup/stack-react": "1.3.43",
    "@leanup/stack-vite": "1.3.43",
    "@leanup/stack-webpack": "1.3.43",
    "@types/react": "17.0.52",
    "@types/react-dom": "17.0.18",
    "@types/react-router": "5.1.13",
    "@types/react-router-dom": "5.1.7",
    "@unocss/preset-mini": "0.48.3",
    "@unocss/vite": "0.48.3",
    "@unocss/webpack": "0.48.3",
    "chromedriver": "97.0.4",
    "copy-webpack-plugin": "8.1.1",
    "cssnano": "5.0.6",
    "npm-check-updates": "12.5.12",
    "postcss-windicss": "1.0.0",
    "selenium-server": "3.141.59",
    "tslib": "2.2.0",
    "typescript": "4.5.5",
    "vite-plugin-replace": "^0.1.1"
  },
  "description": "",
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier --check"
    ]
  }
}
